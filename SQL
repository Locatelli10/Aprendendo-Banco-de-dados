INSERT INTO Ingredientes VALUES (15, pimenta, 7.3);

//Organizando pelo preço
SELECT*FROM Ingredientes ORDER BY preço DESC;

//Selecionando o ingrediente de valor mais alto
SELECT MAX(preço) FROM Ingredientes;
